#include <iostream>

using namespace std;
int main()
{    
     int i=1,count,final;
     int j=20;
    while(i!=0)
    {
        count=0;
        for(int k=1;k<=20;k++)
        {
            if(j%k==0)
            count++;
                    
        }
        //cout<<"\ncount for "<<j<<"is-->"<<count;
        if(count==20)
        {
            i=0;
            break;
        }
        else
        j++;
        
                
       //cout<<"\n"<<j; 
    }
    cout<<j<<endl;
    return 0;
}    
